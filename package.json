{
  "name": "kronos-test-step",
  "version": "0.0.0-semantic-release",
  "description": "kronos-step testing support",
  "main": "index.js",
  "scripts": {
    "doc": "node ./node_modules/.bin/jsdoc lib/*.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "node ./node_modules/.bin/mocha tests/*_test.js",
    "cover": "node ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-test-step.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-test-step/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-test-step#readme",
  "dependencies": {
    "kronos-service": "^3.2.11"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "cz-conventional-changelog": "1.1.6",
    "istanbul": "0.4.3",
    "jsdoc": "3.4.0",
    "mocha": "2.5.3",
    "semantic-release": "6.2.2"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": []
}
