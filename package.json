{
  "name": "kronos-test-step",
  "version": "0.0.0-semantic-release",
  "description": "kronos-step testing support",
  "main": "index.js",
  "scripts": {
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha tests/*_test.js",
    "cover": "npm run prepublish && istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "prepublish": "rollup --output=index.js -c -- {{module}}",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-test-step.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-test-step/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-test-step#readme",
  "dependencies": {
    "kronos-service": "^4.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "semantic-release": "^6.3.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.39.2"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": []
}